{
  "expo": {
    "name": "AsliLearn AI",
    "slug": "aslilearn-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aslilearn.mobile",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2563eb"
      },
      "package": "com.aslilearn.mobile",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "scheme": "aslilearn",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94d0148e-8f4a-4844-b6e8-dbc1c9698e4f"
      }
    },
    "owner": "akhilesh26"
  }
}
